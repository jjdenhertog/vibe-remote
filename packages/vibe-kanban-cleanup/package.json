{
  "name": "@vibe-remote/vibe-kanban-cleanup",
  "version": "7.0.57",
  "description": "Cleanup script for automated PR creation and git operations",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/vibe-kanban-cleanup.mjs",
      "types": "./dist/vibe-kanban-cleanup.d.ts"
    }
  },
  "bin": {
    "vibe-kanban-cleanup": "./dist/vibe-kanban-cleanup.mjs",
    "vibe-start-task": "./dist/start-task-command.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "clean": "rm -rf dist",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "vibe-kanban",
    "cleanup",
    "pr",
    "git",
    "automation",
    "cli",
    "typescript"
  ],
  "dependencies": {
    "@vibe-remote/shared-utils": "workspace:*",
    "@vibe-remote/vibe-kanban-api": "workspace:*"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "tsup": "^8.5.0"
  }
}